program MJProgram
{
    void main()
        int result, switchExpr, temp;
    {
        switchExpr = 2;

        result = 100 + (switch (switchExpr) {
            case 1:
                temp = switch (switchExpr)
                {
                    case 0: yield 40;
                    default: yield 10;
                };
                yield 10;
            case 2:
                temp = switch (switchExpr)
                {
                    case 2: yield 50;
                    default: yield 20;
                };
                yield temp;
            default: yield 30;
        }) + 1;

        result = result * 1000  + (switch (1) {
            case 1:
                temp = switch (-1)
                {
                    case 0: yield 40;
                    default: yield 10;
                };
                yield 10;
            case 2:
                temp = switch (switchExpr)
                {
                    case 2: yield 50;
                    default: yield 20;
                };
                yield temp;
            default: yield 30;
        });

        print(result);
    }
}
