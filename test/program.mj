program MJProgram
{
    void intTest(int x[]) {
        x[0] = 1;
        x[0]++;
        x[1] = 3;
        print(x[0]);
        print(x[1]);
    }

    void charTest(char c[]) {
        print(c[0]);
        print(c[1]);
    }

    void main()
        int x[];
        char c[];
        bool b[];
    {
        x = new int[2];
        intTest(x);
        print(x[1]);

        c = new char[2];
        c[0] = 'a';
        c[1] = 'b';
        charTest(c);

        b = new bool[2];
        b[0] = false;
        b[1] = true;
        print(b[0]);
        print(b[1]);
    }
}
